                    <CheckBox x:Name="checkBox4" Content="Maestro de distritos - Distrito.DBF" HorizontalAlignment="Left" Margin="0,145,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox5" Content="Maestro de efectivos - Efectivo.DBF" HorizontalAlignment="Left" Margin="0,176,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox6" Content="Maestro de infracciones - Infraccion.DBF" HorizontalAlignment="Left" Margin="0,207,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox7" Content="Maestro de licencia - Licencia.DBF" HorizontalAlignment="Left" Margin="0,238,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox8" Content="Maestro de lugares - Lugares.DBF" HorizontalAlignment="Left" Margin="0,269,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox9" Content="Maestro entrega papeletas - MaestraComi.DBF" HorizontalAlignment="Left" Margin="0,300,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox10" Content="Maestro entrega papeletas - MaestraEfec.DBF" HorizontalAlignment="Left" Margin="0,336,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox11" Content="Maestro clase de Vehículos - MasClase.DBF" HorizontalAlignment="Left" Margin="432,31,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox12" Content="Estadisticas infracciones por mes - InfraMes.DBF" HorizontalAlignment="Left" Margin="432,62,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
