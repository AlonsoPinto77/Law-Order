                    <CheckBox x:Name="checkBox14" Content="Parametros del sistema - Sistema.DBF" HorizontalAlignment="Left" Margin="432,114,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
                    <CheckBox x:Name="checkBox15" Content="Maestro talonarios Recepcionados - Talonario.DBF" HorizontalAlignment="Left" Margin="432,145,0,0" VerticalAlignment="Top" Foreground="White" FontSize="18"/>
