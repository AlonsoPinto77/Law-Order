                </Button>
                <Label x:Name="label2" Content="SALIR" HorizontalAlignment="Left" Margin="835,87,0,0" VerticalAlignment="Top" FontWeight="Bold" Foreground="White"/>
            </Grid>
        </Border>
        <Grid Margin="0,146,0,0">
            <GroupBox x:Name="groupBox" Header="" Margin="49,10,44,0" VerticalAlignment="Top" Height="462" Foreground="White" FontSize="24">
                <Grid Margin="10,0,-2,-4">
                    <CheckBox x:Name="checkBox" Content="SalePapeelta.DBF" HorizontalAlignment="Left" Margin="0,31,0,0" VerticalAlignment="Top" Foreground="White" FontSize="28"/>
                    <CheckBox x:Name="checkBox1" Content="InfraccionMes.DBF" HorizontalAlignment="Left" Margin="0,124,0,0" VerticalAlignment="Top" Foreground="White" FontSize="28"/>
                    <CheckBox x:Name="checkBox2" Content="Maestra.DBF" HorizontalAlignment="Left" Margin="10,216,0,0" VerticalAlignment="Top" Foreground="White" FontSize="28"/>
                    <CheckBox x:Name="checkBox3" Content="ClaseVeh.DBF" HorizontalAlignment="Left" Margin="0,316,0,0" VerticalAlignment="Top" Foreground="White" FontSize="28"/>
                    <Label x:Name="label1" Content="Almacena todas las papeletas que se ingresan" HorizontalAlignment="Center" Margin="339,33,24,0" VerticalAlignment="Top" Height="49.75" Width="523" Foreground="White" FontSize="24" BorderThickness="5,0,0,5" UseLayoutRounding="False"/>
                    <Label x:Name="label3" Content=" dia a dia" HorizontalAlignment="Left" Margin="337,60,0,0" VerticalAlignment="Top" Height="50" Width="329" Foreground="White" FontSize="24"/>
                    <Label x:Name="label4" Content=" almacena las fracciones por mes y año, segun " HorizontalAlignment="Left" Margin="339,121,0,0" VerticalAlignment="Top" Height="50" Width="501" Foreground="White" FontSize="24"/>
                    <Label x:Name="label5" Content=" las pepeletas que se ve" HorizontalAlignment="Left" Margin="339,147,0,0" VerticalAlignment="Top" Height="50" Width="329" Foreground="White" FontSize="24"/>
                    <Label x:Name="label6" Content=" Almacena todas las papeletas que se entrega" HorizontalAlignment="Left" Margin="337,202,0,0" VerticalAlignment="Top" Height="50" Width="503" Foreground="White" FontSize="24"/>
                    <Label x:Name="label7" Content=" a Comisarias y efectivos" HorizontalAlignment="Left" Margin="337,229,0,0" VerticalAlignment="Top" Height="50" Width="329" Foreground="White" FontSize="24"/>
                    <Label x:Name="label8" Content=" Almacena todas las papeletas según la clase " HorizontalAlignment="Left" Margin="337,298,0,0" VerticalAlignment="Top" Height="50" Width="503" Foreground="White" FontSize="24"/>
                    <Label x:Name="label9" Content=" del vehiculo por mes y año" HorizontalAlignment="Left" Margin="337,321,0,0" VerticalAlignment="Top" Height="50" Width="329" Foreground="White" FontSize="24"/>
                </Grid>
            </GroupBox>
        </Grid>
    </Grid>
</UserControl>
