<UserControl x:Class="PrototiposPoltran.ActualizacionTabEstadisticas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrototiposPoltran"
             mc:Ignorable="d" 
             Background="#2E2E2E" Width="1006.5" Height="636.5">
    <Grid>
        <Border BorderBrush="White" BorderThickness="4" Margin="29,10,38,490" RenderTransformOrigin="0.498,0.363">
            <Grid Margin="0,6,-8,-4" HorizontalAlignment="Right" Width="933">
                <Image x:Name="logPoltran" HorizontalAlignment="Left" Height="100" Margin="13,11,0,0" VerticalAlignment="Top" Width="100" Source="pnpok.png"/>
                <Label x:Name="label" Content="Actualizacón de Tablas Estadisticas " HorizontalAlignment="Left" Margin="118,36,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold" Foreground="White"/>
                <TextBox x:Name="textBox" Margin="160,31,634,331" TextWrapping="Wrap" FontSize="18"/>
                <Button x:Name="btnProcesar" Content="Procesar" HorizontalAlignment="Left" Margin="608,31,0,0" VerticalAlignment="Top" Width="162" Height="48" Click="btnProcesar_Click"/>
                <Button x:Name="btnSalir"  HorizontalAlignment="Left" Margin="824,15,0,0" VerticalAlignment="Top" Width="74" Height="72" Background="Transparent" Click="btnSalir_Click">
